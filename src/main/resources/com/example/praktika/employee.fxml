        </AnchorPane>
      <ListView fx:id="tour" layoutX="13.0" layoutY="124.0" prefHeight="266.0" prefWidth="574.0" />
      <TextField fx:id="pos" layoutX="70.0" layoutY="84.0" prefHeight="26.0" prefWidth="517.0" promptText="поиск">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </TextField>
      <ImageView fx:id="image1" fitHeight="32.0" fitWidth="35.0" layoutX="14.0" layoutY="84.0" onMouseClicked="#Home" pickOnBounds="true" preserveRatio="true">
         <image>

         </image>
      </ImageView>
    </children>

</AnchorPane>
