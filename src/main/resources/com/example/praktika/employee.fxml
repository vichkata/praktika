                        <Font name="Comic Sans MS" size="15.0" />
                    </font>
                </Button>
            <ComboBox fx:id="climate" editable="true" layoutX="277.0" layoutY="30.0" prefHeight="26.0" prefWidth="96.0" promptText="климат" />
            <ImageView fx:id="image" fitHeight="32.0" fitWidth="26.0" layoutX="534.0" layoutY="30.0" onMouseClicked="#HomePage" pickOnBounds="true" preserveRatio="true">
               <image>

               </image>
            </ImageView>
            <ComboBox fx:id="sea" editable="true" layoutX="415.0" layoutY="29.0" prefHeight="26.0" prefWidth="106.0" promptText="море" />
