                        <Font name="Comic Sans MS" size="15.0" />
                    </font>
                </Button>
                <Button fx:id="main" layoutX="100.0" layoutY="27.0" mnemonicParsing="false" style="-fx-background-color: transparent;" text="забронировать тур">
