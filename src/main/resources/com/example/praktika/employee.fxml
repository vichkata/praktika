<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.praktika.Employee">
    <children>
        <AnchorPane prefHeight="75.0" prefWidth="600.0" style="-fx-background-color: #a8dfa0;">
