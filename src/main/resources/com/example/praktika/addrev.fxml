<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.course.Addrev">
    <children>
        <AnchorPane prefHeight="400.0" prefWidth="294.0" style="-fx-background-color: #a8dfa0;">
            <children>
                <Label layoutX="71.0" layoutY="115.0" text="Добавить отзыв" textFill="WHITE">
                    <font>
                        <Font name="Comic Sans MS" size="20.0" />
                    </font>
                </Label>
                <Button fx:id="insert" layoutX="104.0" layoutY="322.0" mnemonicParsing="false" text="добавить">
                    <font>
                        <Font name="Comic Sans MS" size="15.0" />
                    </font>
                </Button>
            </children>

        </AnchorPane>
        <TextField fx:id="add" layoutX="322.0" layoutY="174.0" prefHeight="26.0" prefWidth="247.0" promptText="впишите ваш отзыв">
            <font>
                <Font name="Comic Sans MS" size="15.0" />
            </font>
        </TextField>
        <TextField fx:id="star" layoutX="322.0" layoutY="245.0" promptText="количество звезд">
            <font>
                <Font name="Comic Sans MS" size="15.0" />
            </font>
        </TextField>
        <ImageView fx:id="home" fitHeight="45.0" fitWidth="46.0" layoutX="541.0" layoutY="18.0" onMouseClicked="#HomePage" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../../../../../Desktop/home.png" />
            </image>
        </ImageView>
    </children>
</AnchorPane>
